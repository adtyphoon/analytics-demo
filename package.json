{
  "name": "analytics-demo",
  "version": "0.2.1",
  "scripts": {
    "build": "yarn build:production",
    "build:development": "NODE_ENV=development webpack",
    "build:production": "NODE_ENV=production webpack",
    "build:watch": "yarn build:development --watch",
    "lint": "yarn lint:js && yarn lint:ts && yarn lint:tsc",
    "lint:fix": "yarn lint:js --fix",
    "lint:fixall": "yarn lint:js --fix && yarn lint:ts --fix",
    "lint:js": "eslint --ignore-path ../../../.gitignore --ext js .",
    "lint:ts": "tslint -c ../../../tslint.json --project tsconfig.json 'src/**/*.ts' 'src/**/*.tsx'",
    "lint:tsc": "tsc --project tsconfig.json",
    "sandbox": "NODE_ENV=development webpack-dev-server --hot --open",
    "start": "yarn sandbox",
    "start:localenv": "yarn clean:localenv && yarn boot:localenv && yarn sandbox",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "files": [
    "public/"
  ],
  "dependencies": {
    "assessment-lib": "0.25.9"
  },
  "devDependencies": {
    "@types/plupload": "2.0.6",
    "classnames": "2.2.5",
    "core-js": "3.1.3",
    "design-system-components": "2.15.0",
    "design-system-components": "2.19.1",
    "design-system-organisms": "1.3.1",
    "lodash": "4.17.10",
    "mobx": "4.5.0",
    "mobx-react": "5.4.4",
    "node-polyglot": "2.3.0",
    "plupload": "2.3.6",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "whatwg-fetch": "2.0.4"
  },
  "license": "UNLICENSED"
}
