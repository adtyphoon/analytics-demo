    "scripts": {
        "tsoa": "rm -f src/http/routes.ts && rm -f src/http/swagger.json && tsoa routes && tsoa swagger",
        "build": "npm run tsoa && tsc",
        "start": "node -r source-map-support/register --optimize_for_size --max_old_space_size=460 --gc_interval=100 build/index.js",
        "startdev": "npm run build && API_KEY=uihtb2l902ib8aoe34gf53ak35b850b24ioua6a835d4 NODE_ENV=local LOG_SETTING=simple node $NODE_DEBUG_OPTION -r source-map-support/register build/index.js",
        "prepare": "npm run build",
        "lint": "tslint -p .",
        "test": "npm audit && npm run lint && LOG_SETTING=simple NODE_ENV=test LOG_LEVEL=warn jest -c jest.all.config.js --runInBand --detectOpenHandles",
        "testcli": "npm audit && npm run lint && LOG_SETTING=simple NODE_ENV=docker LOG_LEVEL=warn jest -c jest.all.config.js --runInBand --detectOpenHandles --coverage --coverageDirectory=output/coverage/jest/",
        "test:nolint": "npm audit && LOG_SETTING=simple NODE_ENV=test LOG_LEVEL=warn jest -c jest.all.config.js --runInBand --detectOpenHandles",
        "test:unit": "npm audit && npm run lint && LOG_SETTING=simple NODE_ENV=test LOG_LEVEL=warn jest -c jest.unit.config.js --detectOpenHandles --runInBand",
        "test:integration": "npm run lint && LOG_SETTING=simple NODE_ENV=test LOG_LEVEL=warn jest -c jest.integration.config.js --runInBand --detectOpenHandles",
        "test:coverage": "npm run lint && LOG_SETTING=simple NODE_ENV=test LOG_LEVEL=warn jest -c jest.all.config.js --coverage --runInBand --detectOpenHandles",
        "migrateZAPItoH2O": "tsc && LOG_SETTING=simple node build/scripts/migrateZapiToH2O.local.js",
        "validateH2OwithZapi": "tsc && LOG_SETTING=simple node build/scripts/validateH2OwithZapi.local.js",
        "heroku-prebuild": "bash preinstall.sh",
        "heroku-postbuild": "bash postinstall.sh",
        "testBreaker": "tsc && LOG_SETTING=simple node build/scripts/breakerPlayground.js"
    },
    "repository": {
        "type": "git",
        "url": "https://www.github.com/danMateer/analytics-demo/"
