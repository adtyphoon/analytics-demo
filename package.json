  "scripts": {
    "start": "node --harmony app.js",
    "start:feature:server": "forever start -c /bin/bash ./test/feature/server",
    "debug": "node debug app.js",
    "dev": "nodemon app.js -- --environment=development",
    "testing": "NODE_ENV=testing npm start",
    "build": "node_modules/.bin/grunt build",
    "build:views": "node scripts/build-views.js",
    "develop": "./dev",
    "develop:console": "node app.js",
    "nsolid": "nsolid app.js",
    "test": "echo 'please use either npm run test:server, test:frontend or test:frontend:ci' && exit 1",
    "test:ipm:server": "NODE_ENV=testing mocha --opts ./packages/mocha.opts",
    "test:ipm:frontend": "TEST_IPM_PACKAGES=true karma start",
    "test:ipm:frontend:ci": "TEST_IPM_PACKAGES=true karma start --single-run",
    "test:ipm:feature": "TEST_IPM_PACKAGES=true npm run test:core:feature ",
    "test:core:server": "NODE_ENV=testing mocha --opts ./test/mocha.opts",
    "test:core:config": "NODE_ENV=testing mocha --opts ./test/mocha.config.opts",
    "test:core:controllers": "NODE_ENV=testing mocha --opts ./test/mocha.controllers.opts",
    "test:core:integration": "NODE_ENV=testing mocha --opts ./test/mocha.integration.opts",
    "test:core:orm": "NODE_ENV=testing mocha --opts ./test/mocha.orm.opts",
    "test:core:services": "NODE_ENV=testing mocha --opts ./test/mocha.services.opts",
    "test:core:frontend": "npm run build && karma start && karma start karma.webpack.config.js --single-run=true",
    "test:core:frontend:agent": "npm run build && USER=agent karma start",
    "test:core:frontend:ci": "npm run build && karma start --single-run",
    "test:core:feature": "wdio wdio.standalone.conf.js",
    "test:server": "npm run test:core:server && npm run test:ipm:server",
    "test:frontend": "npm run test:core:frontend",
    "test:frontend:ci": "npm run test:core:frontend:ci",
    "test:jest:ci": "./node_modules/.bin/jest",
    "test:feature": "npm run test:core:feature && npm run test:ipm:feature",
    "test:selenium": "./test/feature/test",
    "test:all": "npm run test:lint && npm run test:server && npm run build && npm run test:frontend:ci && USER=agent npm run test:frontend:ci && npm run test:selenium",
    "test:lint": "eslint --fix $(git diff-tree --no-commit-id --name-only -r HEAD~0)",
    "test:webpack": "./node_modules/.bin/karma start karma.webpack.config.js",
    "test:webpack:ci": "./node_modules/.bin/karma start karma.webpack.config.js --single-run=true",
    "coffeelint": "coffeelint api/ packages/ assets/",
    "sequelize": "sequelize",
    "migrate:new": "sequelize migration:create",
    "migrate:up": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migration-sync": "node scripts/migration-sync.js",
    "base:develop": "./build-base.sh develop",
    "base:master": "./build-base.sh master",
    "jsdoc:start": "nodemon -x 'jsdoc -c jsdoc.json'",
    "font-sync": "node scripts/s3.js",
    "deploy:assets": "node scripts/deploy-assets.js",
    "protractor": "protractor test/e2e/protractor.conf.js",
    "seed:up": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo:all",
    "permissions:load": "node scripts/load-permissions.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
